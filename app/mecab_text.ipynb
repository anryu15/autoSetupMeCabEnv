    "import MeCab\n",
    "\n",
    "# 解析対象の文字列を用意\n",
    "text = \"リアル二刀流とはなんぞや\"\n",
    "\n",
    "# 辞書を指定してタガー生成\n",
    "tagger = MeCab.Tagger(\"-Ochasen -d /usr/local/lib/mecab/dic/mecab-ipadic-neologd\")\n",
    "\n",
    "# 形態素解析結果を出力\n",
    "for line in tagger.parse(text).splitlines():\n",
    "    print(line)"
   ]
